{
  "parser": "babel-eslint",

  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "valid-typeof": 0
  }
}
